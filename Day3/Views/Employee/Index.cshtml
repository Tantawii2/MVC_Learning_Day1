@using Day3.Dal.Entities
@model List<Employee>


<table class="table table-striped table-hover">


    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Salary</td>
        </tr>
    </thead>
    @foreach (var item in Model)
    {

        <tr>
            <td> @item.Id</td>
            <td> @item.Name</td>
            <td> @item.Salary</td>
        </tr>
    }
</table>


